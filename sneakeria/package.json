{
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "postinstall": "npm run build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test:integration": "jest -c jest.config.integration.js",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "data-gen": "cd api && cd data-gen && node data-gen.js",
    "api-start": "cd api && npm start ",
    "development": "concurrently \"npm run data-gen\" \"npm run api-start\" \"cd client && npm run serve\"",
    "production-build": "concurrently \"npm run data-gen\" \"npm run build\"",
    "production": "concurrently \"npm run api-start\" \"NODE_ENV=production npm run client-start\" "
  },
